		<label>Press MeadowSweet</label>
		<description>Presses MeadowSweet to extract its resource solution.</description>
		<jobString>Pressing MeadowSweet.</jobString>
		<workAmount>120</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workEffect>Cook</workEffect>
    <sustainerSoundDef>Recipe_CookMeal</sustainerSoundDef>
    <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MeadowSweetCrop</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<products>
      <MeadowSweetSolution>1</MeadowSweetSolution>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MeadowSweetCrop</li>
